XF86Audio{Prev,Next}
	mpc -q {prev,next}

@XF86LaunchA
	scrot -s -e 'image_viewer $f'

super + F8
     xbacklight -inc 10

super + F9
    xbacklight -dec 10
super + F5
    sudo amixer set Master playback 5+ 
super + F4
    sudo amixer set Master playback 5-
super + Print
	import /home/chroot/Pictures/chroot+$(date +%T).png
